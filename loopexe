1:

let nota;

do {
  nota = prompt("Digite uma nota entre 0 e 10:");
  nota = parseFloat(nota);

  if (nota < 0 || nota > 10 || isNaN(nota)) {
    alert("Valor inválido, insira uma nota entre 0 e 10.");
  }
} while (nota < 0 || nota > 10 || isNaN(nota));
alert("Sua nota é " + nota);


